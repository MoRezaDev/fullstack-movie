model Post {
    id String @id @default(uuid())
    title String @unique
    slug String @unique
    description String
    is_premium Boolean @default(false)

    extra_info String?
    download_info String?

    watchlist WatchList[]

    movie Movie?

    anime Anime?

    series Series?
    

    download_links DownloadLink[]

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

}
